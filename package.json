{
  "name": "uber-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "active-menu-link": "^1.1.6",
    "create-react-app": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-scroll": "^1.7.12",
    "simple-scrollspy": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rename-index": "node -e \"require('fs').rename('./build/index.html', './build/200.html', (err) => { console.log(err ? err : 'File renamed!') })\"",
    "predeploy": "npm run build && npm run rename-index",
    "deploy": "cd build && surge --domain uber-eats-jukachu.surge.sh",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "1.18.2",
    "surge": "^0.21.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deploy"
    }
  }
}
